var single=document.querySelector('.js-single');var options={rootMargin:'-85px'}
function handler(entries,observer){for(entry of entries){if(entry.isIntersecting){single.classList.remove("has-scrolled");}else{single.classList.add("has-scrolled")}}}
let observer=new IntersectionObserver(handler,options);observer.observe(document.querySelector('.js-single-background'));